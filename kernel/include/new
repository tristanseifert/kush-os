#ifndef KERNEL_NEW_H
#define KERNEL_NEW_H

#include <stddef.h>

inline void *operator new(size_t, void *p)     throw() { return p;  }
inline void *operator new[](size_t, void *p)   throw() { return p;  }
inline void  operator delete  (void *, void *) throw() { };
inline void  operator delete[](void *, void *) throw() { };

#endif
